@article{arango_serna_indicadores_2017,
	title = {Indicadores de desempeño para empresas del sector logístico: Un enfoque desde el transporte de carga terrestre},
	volume = {25},
	issn = {0718-3305},
	url = {https://scielo.conicyt.cl/scielo.php?script=sci_abstract&pid=S0718-33052017000400707&lng=es&nrm=iso&tlng=en},
	doi = {10.4067/S0718-33052017000400707},
	shorttitle = {Indicadores de desempeño para empresas del sector logístico},
	abstract = {:El presente artículo propone un nuevo marco metodológico para el desarrollo de indicadores que permitan medir el desempeño de las actividades clave para cuatro áreas decisionales identificadas en las empresas de transporte de carga terrestre, siendo estas: gerencia, soporte administrativo, operaciones -logística- y tecnología de procesos. La propuesta teórica se basa en un análisis detallado de literatura científica, de este análisis es posible identificar las actividades clave en cada área decisional de las empresas, al igual que los recursos que se ven afectados por variables internas o externas a la operación y que impactan el desempeño empresarial. Como resultado se propone un marco de indicadores para la medición del desempeño de las empresas de transporte de carga que incluye la definición de cada indicador y cómo calcularlo. Finalmente, se concluye que es posible obtener un conjunto de indicadores que se basen en las necesidades identificadas en literatura científica sin obviar las particularidades de las empresas de transporte de carga, lo que asegura la funcionalidad y el éxito de la implementación de estos indicadores.Palabras clave: Indicadores de desempeño; actividades logísticas; transporte terrestre de carga; Logística},
	pages = {707--720},
	number = {4},
	journaltitle = {Ingeniare. Revista chilena de ingeniería},
	author = {Arango Serna, Martín Darío and Ruiz Moreno, Silvana and Ortiz Vásquez, Luisa Fernanda and Zapata Cortes, Julián Andrés and Arango Serna, Martín Darío and Ruiz Moreno, Silvana and Ortiz Vásquez, Luisa Fernanda and Zapata Cortes, Julián Andrés},
	urldate = {2020-07-26},
	date = {2017-12},
	file = {Full Text PDF:/home/chava/Zotero/storage/HIGESVSN/Arango Serna et al. - 2017 - Indicadores de desempeño para empresas del sector .pdf:application/pdf;Snapshot:/home/chava/Zotero/storage/RMMXU6X9/scielo.html:text/html}
}

