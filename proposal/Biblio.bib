@article{hussain_metaheuristic_2019,
	title = {Metaheuristic research: a comprehensive survey},
	volume = {52},
	issn = {0269-2821, 1573-7462},
	url = {http://link.springer.com/10.1007/s10462-017-9605-z},
	doi = {10.1007/s10462-017-9605-z},
	shorttitle = {Metaheuristic research},
	abstract = {Because of successful implementations and high intensity, metaheuristic research has been extensively reported in literature, which covers algorithms, applications, comparisons, and analysis. Though, little has been evidenced on insightful analysis of metaheuristic performance issues, and it is still a “black box” that why certain metaheuristics perform better on speciﬁc optimization problems and not as good on others. The performance related analyses performed on algorithms are mostly quantitative via performance validation metrics like mean error, standard deviation, and co-relations have been used. Moreover, the performance tests are often performed on speciﬁc benchmark functions—few studies are those which involve real data from scientiﬁc or engineering optimization problems. In order to draw a comprehensive picture of metaheuristic research, this paper performs a survey of metaheuristic research in literature which consists of 1222 publications from year 1983 to 2016 (33 years). Based on the collected evidence, this paper addresses four dimensions of metaheuristic research: introduction of new algorithms, modiﬁcations and hybrids, comparisons and analysis, and research gaps and future directions. The objective is to highlight potential open questions and critical issues raised in literature. The work provides guidance for future research to be conducted more meaningfully that can serve for the good of this area of research.},
	pages = {2191--2233},
	number = {4},
	journaltitle = {Artificial Intelligence Review},
	shortjournal = {Artif Intell Rev},
	author = {Hussain, Kashif and Mohd Salleh, Mohd Najib and Cheng, Shi and Shi, Yuhui},
	urldate = {2020-07-31},
	date = {2019-12},
	langid = {english},
	file = {Hussain et al. - 2019 - Metaheuristic research a comprehensive survey.pdf:/home/chava/Zotero/storage/KARYXLZU/Hussain et al. - 2019 - Metaheuristic research a comprehensive survey.pdf:application/pdf}
}